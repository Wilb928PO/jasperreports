<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
	<title>JasperReports Viewer</title>
	<meta http-equiv="Content-Type" content="text/html; charset='UTF-8'"/>
	<style type="text/css">
		a {text-decoration: none}
	</style>
   	<script src='${contextPath}/jquery/js/jquery-1.7.1.min.js'></script>
   	<script src='${contextPath}/jquery/js/jquery-ui-1.8.16.custom.min.js'></script>
   	<script src='${globaljs}'></script>
   	<script>
   		JasperReports.modules.global.APPLICATION_CONTEXT_PATH = '${contextPath}';
   	</script>
   	<link type='text/css' rel='stylesheet' href='${contextPath}/jquery/theme/jquery.ui.all.css'></link> 
   	<link type='text/css' rel='stylesheet' href='${globalcss}'></link>
</head>
<body text="#000000" link="#000000" alink="#000000" vlink="#000000" bgcolor="#999999">
<div class='mainReportDiv' style='width: 100%; height: 100%; overflow: auto'>
#if(${showToolbar})
	<div id='${toolbarId}' class='toolbarDiv' style='position: absolute; width: 400px; height: 40px; background-color: grey; z-index: 999998'
			data-url='${currentUrl}' 
			data-totalpages='' 
			data-currentpage=''> 
		<img class='pageFirst disabledViewerButton' src='${contextPath}/jasperreports/images/first.GIF' />
		<img class='pagePrevious disabledViewerButton' src='${contextPath}/jasperreports/images/previous.GIF' />
		<img class='pageNext disabledViewerButton' src='${contextPath}/jasperreports/images/next.GIF' />
		<img class='pageLast disabledViewerButton' src='${contextPath}/jasperreports/images/last.GIF' />
		<img class='undo disabledViewerButton' src='${contextPath}/jasperreports/images/first.GIF' />
		<img class='redo disabledViewerButton' src='${contextPath}/jasperreports/images/last.GIF' />
		<img class='save disabledViewerButton' src='${contextPath}/jasperreports/images/save.GIF' />
	</div>
	<script type='text/javascript'>
		JasperReports.modules.global.initToolbar('${toolbarId}');
	</script>
#end
<div class='result'>
